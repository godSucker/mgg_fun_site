---
import BaseLayout from '@/layouts/BaseLayout.astro';
import MutantsBrowser from '@/components/MutantsBrowser.svelte';
import mutants from '@/data/mutants/mutants.json';

const data = mutants.filter((m: any) => m.stars?.normal);
const title = 'Обычные мутанты';
---

<BaseLayout title={title}>
  <MutantsBrowser client:load items={data} star="normal" title={title} />
</BaseLayout>
